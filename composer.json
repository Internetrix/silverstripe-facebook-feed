{
  "name": "dexven/facebook-token-converter",
  "description": "Allows creation of long-lived Facebook access tokens to be used in the display of social media feeds.",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/Dexven/facebook-token-converter",
  "keywords": [
      "silverstripe",
      "module",
      "facebook",
      "feed"
  ],
  "license": "BSD-3-Clause",
  "support": {
      "issues": "https://github.com/Dexven/facebook-token-converter/issues"
  },
  "require": {
      "silverstripe/framework": "^4",
      "silverstripe/cms": "^4"
  },
  "require-dev": {
      "phpunit/phpunit": "^5.7",
      "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
      "installer-name": "facebook-token-converter"
  },
  "autoload": {
      "psr-4": {
          "Dexven\\TokenConverter\\": "src/"
      }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}